{
  "name": "redux-websocket-example",
  "version": "0.0.1",
  "author": "Max Nachlinger",
  "license": "ISC",
  "description": "A websocket example",
  "keywords": [],
  "engines": {
    "node": ">=10.0.0 <11.0.0"
  },
  "bugs": {
    "url": "https://github.com/maxnachlinger/redux-websocket-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@githib.com:maxnachlinger/redux-websocket-example.git"
  },
  "scripts": {
    "fmt": "node_modules/.bin/prettier --write '**/*.js*'",
    "build-client-prod": "webpack --config client/build/webpack-prod.js",
    "build-client-dev": "webpack --config client/build/webpack-dev.js",
    "pretest": "npm run fmt",
    "test": "jest",
    "start-dev": "concurrently --kill-others 'npm run build-client-dev' 'node ./server/server.js'",
    "start-prod": "npm run build-client-prod && node ./server/server.js"
  },
  "dependencies": {
    "bufferutil": "4.0.1",
    "express-session": "1.15.6",
    "fs-extra": "7.0.1",
    "helmet": "3.15.1",
    "level": "4.0.0",
    "level-session-store": "2.2.0",
    "polka": "0.5.2",
    "serve-static": "1.13.2",
    "utf-8-validate": "5.0.2",
    "uuid": "3.3.2",
    "ws": "6.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "assets-webpack-plugin": "3.9.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "browserslist": "4.4.1",
    "clean-webpack-plugin": "1.0.1",
    "compression-webpack-plugin": "2.0.0",
    "concurrently": "4.1.0",
    "css-loader": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "preact": "8.4.2",
    "preact-compat": "3.18.4",
    "preact-redux": "2.0.3",
    "prettier": "1.16.4",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-cli": "3.2.3"
  }
